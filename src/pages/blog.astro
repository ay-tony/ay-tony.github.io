---
import BaseLayout from "/src/layouts/base-layout.astro"

const mdPosts = Object.values(import.meta.glob('./posts/*.md', { eager: true }));
const pageTitle = "文章";
---

<BaseLayout pageTitle={pageTitle}>
    <ul>
        {mdPosts.map((post: any) => <li><a href={post.url}>{post.frontmatter.title}</a></li>)}
    </ul>
</BaseLayout>
